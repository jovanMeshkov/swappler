<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://swappler.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>swappler.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>swappler</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$swappler</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="SwapplerSqliteContext" Order="2" Enabled="False">
          <Destination Path="Data Source=VISTA-JOVAN\SQLSERVER2014;Initial Catalog=Testiranje_DB_test;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Swappler.Database.SwapplerSqliteContext, Swappler" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <_DestinationType>AzureWebSite</_DestinationType>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)SwapplerSqliteContext-Web.config Connection String">
      <ParameterValue>metadata=res://*/Database.SwapplerEntityDataModel.csdl|res://*/Database.SwapplerEntityDataModel.ssdl|res://*/Database.SwapplerEntityDataModel.msl;provider=System.Data.SQLite.EF6;provider connection string="Data Source=VISTA-JOVAN\SQLSERVER2014;Initial Catalog=Testiranje_DB_test;Integrated Security=True"</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>